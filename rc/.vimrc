set nocompatible
set number
set mouse=
syntax on
set tabstop=4
set shiftwidth=4
set expandtab
set smartindent
set autoindent
set clipboard=unnamedplus
set nowrap
set incsearch
set hlsearch
set ignorecase
set smartcase
set showcmd
set cursorline
set wildmenu
set lazyredraw
set hidden
set numberwidth=1
highlight Normal ctermbg=black ctermfg=white
highlight LineNr ctermbg=black ctermfg=darkgray
filetype plugin indent on

